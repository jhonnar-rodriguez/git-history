  "description": "Easily track changes across repositories, manage dependencies, and ensure that all parts of your project are working together seamlessly for our git-tree app.",
  "keywords": [
    "turbo",
    "nestjs",
    "vite",
    "react",
    "airbnb",
    "eslint"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint --",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test --",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.5.1",
    "turbo": "^1.9.3"
  },
  "name": "github-history",
  "packageManager": "npm@9.6.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
